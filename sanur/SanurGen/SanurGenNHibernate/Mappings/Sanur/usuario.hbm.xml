<?xml version="1.0" encoding="utf-8" ?>
		<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
		                   assembly="SanurGenNHibernate"
		                   namespace="SanurGenNHibernate.EN.Sanur">
			<class name="UsuarioEN" table="[Usuario]">
				  	
						<id name="IdUsuario" column="[idUsuario]" type="integer">
							<generator class="identity"/>
						</id>
					<property name="Nombre" type="string"> 
						<column name="[nombre]"  not-null="true"/>
					</property>
					<property name="Contrasena" type="string"> 
						<column name="[contrasena]"  not-null="true"/>
					</property>
					<property name="Iniciado" type="boolean"> 
						<column name="[iniciado]"  not-null="true"/>
					</property>
					<property name="Email" type="string"> 
						<column name="[email]"  unique="true" not-null="true"/>
					</property>
					<property name="Apellidos" type="string"> 
						<column name="[apellidos]"  not-null="true"/>
					</property>
			
			
			
			
			
			
					
					
					
					
					
					
		
		  </class>
		  
		  <query name="UsuarioENReadAllHQL">from UsuarioEN</query>
		  <query name="UsuarioENreadMailHQL"><![CDATA[FROM UsuarioEN  as usu where usu.Email = :p_mail]]></query>
		  <query name="UsuarioENusuarioPorMailHQL"><![CDATA[FROM UsuarioEN  as usu where usu.Email = :p_mail]]></query>
		  <query name="UsuarioENusuarioPorNombreHQL"><![CDATA[FROM UsuarioEN  as usu where usu.Nombre = :p_nombre]]></query>
		</hibernate-mapping>
	